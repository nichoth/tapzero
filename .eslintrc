{
  "parserOptions": {
    "requireConfigFile": false
  },
  "extends": [
    "standard"
  ],
  "plugins": [],
  "rules": {
    "operator-linebreak": ["off"],
    "multiline-ternary": "off",
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "indent": ["error", 4, {
      "SwitchCase": 1,
      "ignoredNodes": ["TemplateLiteral *"]
    }],
    "comma-dangle": "off",
    "no-multi-spaces": ["error", { "ignoreEOLComments": true }]
  }
}
